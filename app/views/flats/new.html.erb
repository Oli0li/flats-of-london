<h1>Rent your property</h1>

<%= simple_form_for(@flat) do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :description %>
  <%= f.input :price_per_night %>
  <%= f.input :number_of_guests %>
  <%= f.submit %>
<% end %>

<%= link_to "Back to all flats", flats_path, class: "btn btn-primary mt-3" %>
